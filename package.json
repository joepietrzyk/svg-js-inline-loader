{
  "name": "svg-js-inline-loader",
  "version": "0.1.0",
  "description": "A Webpack Loader that inlines your SVG files into as a JavaScript function. Turns them into a function that returns an SVGElement.",
  "keywords": [
    "svg",
    "webpack",
    "loader"
  ],
  "author": "Joseph Pietrzyk <joepietrzyk@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/joepietrzyk/svg-js-inline-loader"
  },
  "bugs": {
    "url": "https://github.com/joepietrzyk/svg-js-inline-loader/issues"
  },
  "homepage": "https://github.com/joepietrzyk/svg-js-inline-loader",
  "type": "module",
  "module": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint",
    "format": "prettier --write .",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "test": "jest",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@types/webpack": "^5.28.5",
    "fast-xml-parser": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.10.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0"
  }
}
